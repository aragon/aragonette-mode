{
  "name": "mode-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check .; echo 'To write the changes: bun run format:fix'",
    "format:fix": "prettier -w . --list-different",
    "deploy-dao": "bun ./scripts/deploy.ts",
    "prepare": "husky",
    "test": "bun test"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@aragon/ods": "1.0.24",
    "@hookform/resolvers": "^3.9.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@shazow/whatsabi": "^0.14.1",
    "@tanstack/query-async-storage-persister": "^5.56.2",
    "@tanstack/query-sync-storage-persister": "^5.56.2",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-devtools": "^5.58.0",
    "@tanstack/react-query-persist-client": "^5.56.2",
    "@web3modal/wagmi": "^5.1.9",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.6",
    "multiformats": "^13.3.0",
    "next": "14.2.13",
    "react": "18.3.1",
    "react-blockies": "^1.4.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.53.0",
    "recharts": "^2.12.7",
    "viem": "^2.21.14",
    "wagmi": "2.12.14"
  },
  "devDependencies": {
    "@aragon/osx-commons-configs": "^0.7.0-alpha.1",
    "@types/bun": "^1.1.10",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.7.0",
    "@types/react": "^18.3.9",
    "@types/react-blockies": "^1.4.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "tabWidth": 2,
    "arrowParens": "always",
    "useTabs": false,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
