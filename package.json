{
  "name": "aragonette",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=dev bun env-checker.ts && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check .; echo 'To write the changes: bun run format:fix'",
    "format:fix": "prettier -w . --list-different",
    "deploy-dao": "bun ./scripts/deploy.ts",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "prestart": "bun env-checker.ts"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@aragon/ods": "1.0.40",
    "@ethersproject/providers": "^5.7.2",
    "@googleapis/calendar": "^9.7.6",
    "@hookform/resolvers": "^3.6.0",
    "@prisma/client": "^5.14.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@shazow/whatsabi": "0.14.0",
    "@snapshot-labs/snapshot.js": "^0.12.2",
    "@tanstack/query-async-storage-persister": "^5.28.8",
    "@tanstack/react-query": "^5.28.8",
    "@tanstack/react-query-devtools": "^5.29.2",
    "@tanstack/react-query-persist-client": "^5.28.8",
    "@tiptap/extension-code-block": "^2.5.0",
    "@tiptap/extension-image": "^2.5.0",
    "@tiptap/extension-link": "^2.5.0",
    "@tiptap/extension-placeholder": "^2.5.0",
    "@tiptap/pm": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@vercel/analytics": "^1.3.1",
    "@vercel/functions": "^1.4.0",
    "@vercel/kv": "^1.0.1",
    "@vercel/og": "^0.6.2",
    "@vercel/speed-insights": "^1.0.12",
    "@web3modal/wagmi": "^5.0.3",
    "ajv": "^8.14.0",
    "arweave": "^1.15.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "dompurify": "^3.0.11",
    "envalid": "^8.0.0",
    "js-yaml": "^4.1.0",
    "next": "^14.2.5",
    "prisma": "^5.14.0",
    "react": "^18.3.1",
    "react-blockies": "^1.4.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.0",
    "recharts": "^2.12.7",
    "tailwindcss-fluid-type": "^2.0.6",
    "tiptap-markdown": "^0.8.10",
    "viem": "^2.9.2",
    "wagmi": "^2.10.10",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aragon/osx-commons-configs": "^0.2.0",
    "@types/dompurify": "^3.0.5",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.71",
    "@types/react-blockies": "^1.4.4",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "tabWidth": 2,
    "arrowParens": "always",
    "useTabs": false,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
